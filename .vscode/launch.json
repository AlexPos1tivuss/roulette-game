{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch NestJS",
      "program": "${workspaceFolder}/src/main.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      "sourceMaps": true,
      "restart": true,
      "console": "integratedTerminal",
      "autoAttachChildProcesses": true,
      "env": {
        "NODE_ENV": "development"
      }
    }
  ]
}
